#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Project/web_platform_single/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/Project/web_platform_single/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/xml-js@1.6.11/node_modules/xml-js/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/xml-js@1.6.11/node_modules/xml-js/bin/cli.js" "$@"
fi
