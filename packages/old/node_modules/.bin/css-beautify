#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Company_project/Vue/web_platform_single/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/Company_project/Vue/web_platform_single/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/bin/css-beautify.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/js-beautify@1.14.7/node_modules/js-beautify/js/bin/css-beautify.js" "$@"
fi
